#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;

void main ()
{   int a;
	cout<<"Escribir un numero \n";
	cin>>a;
	if (a>=0)
		if (a==0)
			cout<<"Es cero";
	    else 
			cout<<"El numero es positivo \n";
		
	else
		cout<<"El numero es negativo";
	getch();
}
